{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "globals": {
    "__dirname": "readonly"
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react-refresh/only-export-components": "off"
  }
}
